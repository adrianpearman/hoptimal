<div class="users-form">
<%= form_tag sessions_path do %>

  <div class="form-group">
    <%= label_tag :username %>
    <%= text_field_tag :username, params[:username] %>
  </div>

  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>

  <%= submit_tag "Log in", class: "btn btn-default btn-primary" %>

<% end %>
</div>
